    "rollup-plugin-postcss": "^4.0.2",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "postcss": "^8.4.32",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.2"
